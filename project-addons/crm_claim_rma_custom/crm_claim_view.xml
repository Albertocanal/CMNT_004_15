<?xml version="1.0"?>
<openerp>
    <data>

        <record model="ir.ui.view" id="crm_claim_rma_form_view">
            <field name="name">CRM - Claim product return Form</field>
            <field name="model">crm.claim</field>
            <field name="inherit_id" ref="crm_claim_rma.crm_claim_rma_form_view"/>
            <field name="arch" type="xml">
                <field name="email_from" position="after">
                    <field name="agent_id"/>
                </field>
                <page string="Product Return" position="attributes">
                    <attribute name="string">Product Returns</attribute>
                </page>
                <separator string="Product Return" position="attributes">
                    <attribute name="string">Product Returns</attribute>
                </separator>
            </field>
        </record>

    </data>
</openerp>
